<script type="text/javascript">
  // temporary band-aid fix for https://issues.dowjones.net/browse/BLOG-911
  // filters on mobile need to be dealt with properly
  function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
      var c = ca[i];
      while (c.charAt(0)==' ') c = c.substring(1,c.length);
      if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
  }
  var cook = readCookie("DJCOOKIE");
  if ( cook ) {
    if ( cook.match(/SS-\d*-\d*_SS_FILTERS%3D\w*/i) ) { // e.g. SS-#-####_SS_FILTERS=laksjdf
      cook = cook.replace(/SS-\d*-\d*_SS_FILTERS%3D\w*/gi, "");
    }
    // since values in this cookie are separated with ||'s, we probably have duplicates now
    while ( cook.match(/%7C%7C%7C%7C/i) ) {
      cook = cook.replace(/%7C%7C%7C%7C/gi, "%7C%7C");
    }
    document.cookie = "DJCOOKIE="+cook+"; path=/; domain=.wsj.com";
  }
</script>

