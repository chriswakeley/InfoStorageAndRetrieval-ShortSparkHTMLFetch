<script type="text/javascript">
        function hours_difference_between_NY_and_UTC() {
                // Calculating UTC offset in NY because publishing happens in NY
                // Using momentjs
                var now = new Date(); // current time according to client browser - stored in UTC
                var momentobject = moment(now);
                var nytime = momentobject.clone().tz("America/New_York");
                var utcoffsethoursNY = nytime.utcOffset()/(-60);
                return utcoffsethoursNY;
        }
</script>

